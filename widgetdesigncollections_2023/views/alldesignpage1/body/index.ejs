<script nonce="<%= nonce %>">
  document.querySelector(`[HTMLINCLUDE="progressdesignid"]`).style.display =
    "none";
  document.querySelector(`[HTMLINCLUDE="drawermenudesignid"]`).style.display =
    "none";
  document.querySelector(`[HTMLINCLUDE="rangesliderdesignid"]`).style.display =
    "none";
  document.querySelectorAll(".sidenavid1").forEach((button) => {
    button.addEventListener("click", () => {
      document.querySelector(`[HTMLINCLUDE="basicdesignsid"]`).style.display =
        "block";
      document.querySelector(`[HTMLINCLUDE="progressdesignid"]`).style.display =
        "none";
      document.querySelector(
        `[HTMLINCLUDE="drawermenudesignid"]`
      ).style.display = "none";
      document.querySelector(
        `[HTMLINCLUDE="rangesliderdesignid"]`
      ).style.display = "none";
      document.querySelector(`#sidenavid1`).click();
    });
  });

  document.querySelectorAll(".sidenavid2").forEach((button) => {
    button.addEventListener("click", () => {
      document.querySelector(`[HTMLINCLUDE="progressdesignid"]`).style.display =
        "block";
