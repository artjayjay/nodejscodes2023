        "none";
      document.querySelector(
        `[HTMLINCLUDE="drawermenudesignid"]`
      ).style.display = "none";
      document.querySelector(
        `[HTMLINCLUDE="rangesliderdesignid"]`
      ).style.display = "none";
      document.querySelector(`#sidenavid2`).click();
    });
  });

  document.querySelectorAll(".sidenavid3").forEach((button) => {
    button.addEventListener("click", () => {
      document.querySelector(
        `[HTMLINCLUDE="drawermenudesignid"]`
      ).style.display = "block";
      document.querySelector(`[HTMLINCLUDE="basicdesignsid"]`).style.display =
        "none";
      document.querySelector(`[HTMLINCLUDE="progressdesignid"]`).style.display =
        "none";
      document.querySelector(
        `[HTMLINCLUDE="rangesliderdesignid"]`
      ).style.display = "none";
      document.querySelector(`#sidenavid3`).click();
    });
  });
  document.querySelectorAll(".sidenavid4").forEach((button) => {
    button.addEventListener("click", () => {
      document.querySelector(
        `[HTMLINCLUDE="rangesliderdesignid"]`
      ).style.display = "block";
      document.querySelector(
        `[HTMLINCLUDE="drawermenudesignid"]`
      ).style.display = "none";
      document.querySelector(`[HTMLINCLUDE="basicdesignsid"]`).style.display =
        "none";
      document.querySelector(`[HTMLINCLUDE="progressdesignid"]`).style.display =
        "none";
      document.querySelector(`#sidenavid4`).click();
    });
  });
</script>
